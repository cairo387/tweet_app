<div class="main users-new">
  <div class="container">
    <div class="form-heading">ログイン</div>
    <div class="form users-form">
      <div class="form-body">
        <%= form_with model: @user, url: login_user_path, local:true do |f| %>
        <p>メールアドレス</p>
        <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"textfield"%>
        <p>パスワード</p>
        <%= f.password_field :password, autofocus:true, autocomplete: "password", class:"textfield" %>
        <%=f.submit "ログイン" %>
        <% end %>
      </div>
    </div>
  </div>
</div>