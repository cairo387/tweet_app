<div class="main users-index">
  <div class="container">
    <h1 class="users-heading">ユーザー一覧</h1>
    <% @users.each do |user| %>
      <div class="users-index-item">
        <div class="user-left">
          <%= attachment_image_tag(user, :image, format:'jpeg') %>
        </div>
        <div class="user-right">
          <%= link_to user.name, user_path(user) %>
        </div>
      </div>
    <% end %>
  </div>
</div>